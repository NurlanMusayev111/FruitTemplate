@model FactDetailVM


@{
    ViewData["Title"] = "Detail";
}


<div class="container mt-5">
    <form class="forms-sample" method="post" asp-action="Create">
        <div class="form-group">
            <label>Fact Icon</label>
            <input asp-for="Icon" type="text" disabled class="form-control">

            <label>Fact Title</label>
            <input asp-for="Title" type="text" disabled class="form-control">

            <label>Fact Count</label>
            <input asp-for="Count" type="text" disabled class="form-control">

        </div>
        <a asp-action="Index" class="btn btn-primary text-white">back</a>
    </form>
</div>


